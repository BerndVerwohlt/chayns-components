{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties",
        "babel-plugin-optimize-clsx",
        [
            "transform-rename-import",
            {
                "original": "^(.+)\\.scss$",
                "replacement": "$1.css"
            }
        ]
    ]
}
