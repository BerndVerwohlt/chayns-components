    "abortcontroller-polyfill": "^1.4.0",
    "babel-regenerator-runtime": "^6.5.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.2",
    "coordinate-parser": "^1.0.3",
    "eslint-config-airbnb-base": "^14.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
